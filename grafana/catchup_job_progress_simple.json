  "panels": [
    {
      "type": "timeseries",
      "title": "Catchup Segment Progress",
      "datasource": {
        "type": "prometheus",
        "uid": "magi07-prometheus"
      },
      "gridPos": {
        "h": 9,
        "w": 16,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "catchup_segments_claimed_total",
          "legendFormat": "claimed"
        },
        {
          "refId": "B",
          "expr": "catchup_segments_completed_total",
          "legendFormat": "completed"
        },
        {
          "refId": "C",
          "expr": "catchup_segments_retry_wait_total + catchup_segments_dead_letter_total",
          "legendFormat": "errors(total)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "type": "stat",
      "title": "Progress %",
      "datasource": {
        "type": "prometheus",
        "uid": "magi07-prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "catchup_progress_percent"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 90
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "value"
      }
    },
    {
      "type": "stat",
      "title": "Pending Segments",
      "datasource": {
        "type": "prometheus",
        "uid": "magi07-prometheus"
      },
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 16,
        "y": 4
      },
      "targets": [
        {
          "refId": "A",
          "expr": "catchup_pending_segments"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "value"
      }
    },
    {
      "type": "timeseries",
      "title": "Durable Throughput (IDs/sec)",
      "datasource": {
        "type": "prometheus",
        "uid": "magi07-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(catchup_durable_items_total[5m])",
          "legendFormat": "ids/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Failure Ratios",
      "datasource": {
        "type": "prometheus",
        "uid": "magi07-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(catchup_segments_retry_wait_total[5m]) / clamp_min(rate(catchup_segments_claimed_total[5m]), 1)",
          "legendFormat": "retry ratio"
        },
        {
          "refId": "B",
          "expr": "rate(catchup_segments_dead_letter_total[5m]) / clamp_min(rate(catchup_segments_claimed_total[5m]), 1)",
          "legendFormat": "dead-letter ratio"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
  ],
